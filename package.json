{
  "name": "indoqa-react-redux-ts",
  "version": "1.0.0",
  "description": "Indoqa React/Redux Typescript Archetype",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:Indoqa/indoqa-react-redux.git"
  },
  "scripts": {
    "analyze": "source-map-explorer ./target/assets/vendors-*",
    "build": "indoqa-webpack ./indoqa-webpack-options.js",
    "clean": "rimraf ./target",
    "compile": "tsc --outdir ./target/js",
    "lint": "tslint --project .",
    "package": "yarn clean && yarn compile && yarn lint && yarn build",
    "postinstall": "yarn run package",
    "start": "indoqa-dev-server ./indoqa-webpack-options.js"
  },
  "dependencies": {
    "deepmerge": "^2.1.1",
    "fela": "^6.1.8",
    "fela-monolithic": "^5.0.22",
    "fela-plugin-named-media-query": "^5.0.13",
    "fela-preset-web": "^8.0.7",
    "formik": "^1.2.0",
    "history": "^4.7.2",
    "i18next": "^11.7.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "react": "^16.5.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.5.0",
    "react-fela": "^7.3.1",
    "react-i18next": "7.11.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "reselect": "^3.0.1",
    "rxjs": "^6.3.2",
    "shortid": "^2.2.13",
    "typesafe-actions": "^2.0.4",
    "yup": "0.26.3"
  },
  "devDependencies": {
    "@types/deepmerge": "^2.1.0",
    "@types/history": "^4.7.0",
    "@types/i18next": "^11.9.0",
    "@types/i18next-browser-languagedetector": "^2.0.1",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.0.7",
    "@types/react-i18next": "^7.8.2",
    "@types/react-redux": "^6.0.7",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-router-redux": "^5.0.15",
    "@types/redux-logger": "^3.0.6",
    "@types/webpack-env": "^1.13.6",
    "indoqa-webpack": "next",
    "redux-devtools-extension": "^2.13.5",
    "rimraf": "^2.6.2",
    "source-map-explorer": "1.6.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "proxy": {
    "/geonames": {
      "target": "http://api.geonames.org",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "pathRewrite": {
        "/geonames": ""
      }
    },
    "/words": {
      "target": "http://api.wordnik.com",
      "pathRewrite": {
        "/words": ""
      }
    }
  }
}
